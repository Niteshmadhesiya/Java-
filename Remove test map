import java.util.*;
class Testmap
{
public static void main(String[] args)
{
HashMap<Integer,String>map=new
HashMap<Integer,String>();
map.put(1,"A");
map.put(2,"B");
map.put(3,"C");
map.put(4,"D");
map.remove(1);
Set<Integer> keys=map.keySet();
for(Integer i:keys)
{
System.out.println(i+" "+map.get(i));
}
}
}
